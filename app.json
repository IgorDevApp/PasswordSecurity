{
  "expo": {
    "name": "SecurityPass",
    "slug": "SecurityPass",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.IgorDevApp.SecurityPass",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptiveicon.png",
        "backgroundColor": "#999999"
      },
      "permissions":["USE_FINGERPRINT", "USE_BIOMETRIC" ]  
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }

